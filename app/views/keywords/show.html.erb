<div class="container">
  <div class="row">
    <div class="col-md-10 col-md-offset-1">
      <p id="notice"><%= notice %></p>
      <div class="panel panel-default panel-admin" style="margin-top:100px;">
        <div class="panel-heading">
          <%= link_to 'Edit', edit_keyword_path(@keyword) %> |
          <%= link_to 'Back', keywords_path %>
        </div>
        <div class="panel-body">
          <div class="row">
            <div class="col-sm-12">
              <strong>Nama:</strong>
              <%= @keyword.nama %>
            </div>
            <div class="col-sm-12">
              <strong>Bencana:</strong>
              <%= @keyword.bencana %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
